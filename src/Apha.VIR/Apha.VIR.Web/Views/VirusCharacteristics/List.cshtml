@model VirusCharacteristicsViewModel
@{
	ViewData["Title"] = "View Virus Characterstics";
}


<div class="page_links">
	<ul class="ps-1">
		<li>
			<a asp-controller="VirusCharacteristics" asp-action="Create"> Add a new Virus Characteristic </a>
		</li>
		
	</ul>
</div>



<p>Edit an existing Virus Characteristic by clicking on the links below.</p>

<div id="listEntries" class="mt-3">
	<partial name="_VirusCharatersticsList" for="@Model" />
</div>

@section Scripts {
	<script>
		$(document).on('click','.pagination a', function (e) {
			e.preventDefault();
			var pageNo=$(this).data('pageno');
			$.get('BindCharacteristicsGridOnPagination', {pageNo: pageNo, pageSize:10}, function (htmlData) {
			$('#listEntries').html(htmlData);
			});
			});
	</script>
}