@model IEnumerable<Apha.VIR.Web.Models.IsolateRelocation>

@if (Model.Any())
{
    foreach (var isolate in Model)
    {
        <tr>
            <td>@isolate.AVNumber</td>
            <td>@isolate.Nomenclature</td>
            <td>@isolate.FreezerName</td>
            <td>@isolate.TrayName</td>
            <td>@isolate.Well</td>
            <td><input type="checkbox" class="select-isolate" data-id="@isolate.ID" /></td>
            <td><button class="btn btn-link isolaterelocate-btn-link-custom edit-isolate" data-id="@isolate.ID">Edit</button></td>
        </tr>
    }
}
