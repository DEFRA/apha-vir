{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "VIRConnectionString": "Data Source=<serverName>;Initial Catalog=<databaseName>;Integrated Security=True;Encrypt=False;",
        "RedisConnectionString": "<redisendpoint>:<port>,Ssl = true"
    },
    "AzureAd": {
        "Instance": "https://login.microsoftonline.com/",
        "Domain": "<domain_name>.com",
        "ClientId": "<client_Id>",
        "TenantId": "<tenant_Id>",
        "ClientSecret": "<client_Secret>",
        "ClientCertificates": [
        ],
        "CallbackPath": "/signin-oidc"
    },
    "URL": {
        "LogMonitor": "https://defra-cloud.awsapps.com/start/#/console?account_id=<awsaccount>&role_name=<awsrole>&destination=https%3A%2F%2Feu-west-2.console.aws.amazon.com%2Fcloudwatch%2Fhome%3Fregion%3Deu-west-2%23dashboards",
        "UserMgmt": "https://entra.microsoft.com/#view/Microsoft_AAD_IAM/GroupsList.ReactView/queryMode/search/query/ag-azure-vir"
    },
    "ExceptionTypes": {
        "General": "VIR.GENERAL_EXCEPTION",
        "Sql": "VIR.SQL_EXCEPTION",
        "Authorization": "VIR.AUTHORIZATION_EXCEPTION"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning",
                "Microsoft.AspNetCore": "Warning"
            }
        }
    }
}
